--error_model="torque" --log_dir="ros_pnc2/FFNN/caseall_latent256_layer3" --save_model_path="/saved_model" \
--learning_rate=1e-3 --batch_size=64 --epoch_size=300 --latent_size=256 --output_size=12 --num_layer=3


######################################################
#                       validation                   #
######################################################

python3 main_check_model_ffnn.py \
--batch_size=64 \
--dataset_path="a_dataset/tfData/ros_pnc2/FFNN/case1"  \
--test_dataset_path="a_dataset/tfData/ros_pnc2/FFNN/case2" \
--load_model_path="a_result/ros_pnc2/FFNN/case1_latent256_layer3/saved_model" \
--log_dir="check_model/ros_pnc2/FFNN/case1_test2_latent256_layer3" \
--latent_size=256 \
--output_size=12 \
--num_layer=3

python3 main_check_model_ffnn.py \
--batch_size=64 \
--dataset_path="a_dataset/tfData/ros_pnc2/FFNN/case2"  \

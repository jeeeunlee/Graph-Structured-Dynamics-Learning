--error_model="torque" --log_dir="ros_pnc2/FFNN/case3_latent256_layer3" --save_model_path="/saved_model" \
--learning_rate=1e-3 --batch_size=64 --epoch_size=300 --latent_size=256 --output_size=12 --num_layer=3

python3 main_train_model_ffnn.py --dataset_path="a_dataset/tfData/ros_pnc2/FFNN/case4"  \
--error_model="torque" --log_dir="ros_pnc2/FFNN/case4_latent256_layer3" --save_model_path="/saved_model" \
--learning_rate=1e-3 --batch_size=64 --epoch_size=300 --latent_size=256 --output_size=12 --num_layer=3


--error_model="torque" --log_dir="ros_pnc2/FFNN/case7_latent256_layer3" --save_model_path="/saved_model" \
--learning_rate=1e-3 --batch_size=64 --epoch_size=300 --latent_size=256 --output_size=12 --num_layer=3


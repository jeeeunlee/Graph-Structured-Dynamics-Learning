#!/bin/bash

######################################################
#                       train                        #
######################################################

python3 main_train_model.py --dataset_path="a_dataset/tfData/ros_pnc/gnn/caseall"  \
--error_model="torque" --log_dir="ros_pnc/gnn/caseall_edge64_np2" --save_model_path="/saved_model" \
--learning_rate=1e-3 --batch_size=64 --epoch_size=400 --num_processing_steps=2 --edge_latent_size=64 --edge_output_size=3

python3 main_train_model.py --dataset_path="a_dataset/tfData/ros_pnc/gnn/case1"  \
--error_model="torque" --log_dir="ros_pnc/gnn/case1_edge64_np2" --save_model_path="/saved_model" \
--learning_rate=1e-3 --batch_size=64 --epoch_size=400 --num_processing_steps=2 --edge_latent_size=64 --edge_output_size=3


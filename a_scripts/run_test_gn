######################################################
#                       validation                   #
######################################################

python3 main_check_model.py \
--batch_size=64 \
--dataset_path="a_dataset/tfData/ros_pnc/gnn/case1"  \
--test_dataset_path="a_dataset/tfData/ros_pnc/gnn/case2" \
--load_model_path="a_result/ros_pnc/gnn/case1_edge64_np2/saved_model" \
--log_dir="check_model/ros_pnc/gnn/case1_test2_edge64_np2" \
--num_processing_steps=2 \
--edge_latent_size=64 \
--edge_output_size=3

python3 main_check_model.py \
--batch_size=64 \
--dataset_path="a_dataset/tfData/ros_pnc/gnn/case1"  \
--test_dataset_path="a_dataset/tfData/ros_pnc/gnn/case3" \
--load_model_path="a_result/ros_pnc/gnn/case1_edge64_np2/saved_model" \
--log_dir="check_model/ros_pnc/gnn/case1_test3_edge64_np2" \
--num_processing_steps=2 \
--edge_latent_size=64 \
--edge_output_size=3

